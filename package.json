{
  "name": "shibaideman",
  "version": "1.2.0",
  "description": "Shibaider man versatile crawler",
  "main": "index.js",
  "scripts": {
    "build": "node script/build.js",
    "start": "node script/webserver.js",
    "lint": "eslint src --ext .ts --ext .tsx --fix",
    "test": "node script/test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pierrejacques/pilipili.git"
  },
  "keywords": [
    "bilibili",
    "bombPea"
  ],
  "author": "pierrejacques@126.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pierrejacques/pilipili/issues"
  },
  "homepage": "https://github.com/pierrejacques/pilipili#readme",
  "dependencies": {
    "antd": "^4.17.2",
    "classnames": "^2.3.0",
    "csv-parse": "^5.3.4",
    "csv-stringify": "^6.2.3",
    "localforage": "^1.10.0",
    "react": "^17.0.0",
    "react-app-polyfill": "^2.0.0",
    "react-dom": "^17.0.0",
    "rxjs": "^7.4.0",
    "typescript": "^4.5.2"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.181",
    "@types/jest": "^26.0.23",
    "@types/node": "^16.11.11",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-redux": "^7.1.0",
    "@types/webpack-env": "^1.14.0",
    "babel-jest": "^27.0.2",
    "babel-preset-react-app": "^10.0.0",
    "clean-webpack-plugin": "^1.0.0",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "2.1.0",
    "eslint": "^5.16.0",
    "file-loader": "3.0.1",
    "fs-extra": "7.0.1",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "jest": "^27.0.4",
    "jest-resolve": "^27.0.4",
    "jest-watch-typeahead": "^0.6.4",
    "jsdom": "^16.6.0",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "prettier": "^1.18.2",
    "react-hot-loader": "^4.6.3",
    "style-loader": "^0.23.1",
    "ts-loader": "^6.0.4",
    "webpack": "4.28.4",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "3.1.14",
    "write-file-webpack-plugin": "4.5.0"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy",
      "@/(.*)$": "<rootDir>/src/$1"
    },
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx",
      "json",
      "jsx"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  }
}